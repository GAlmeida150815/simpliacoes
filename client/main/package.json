  "dependencies": {
    "bootstrap": "4.6.2",
    "classnames": "2.3.2",
    "date-fns": "^3.6.0",
    "env-cmd": "^10.1.0",
    "express": "^4.19.2",
    "hashids": "^2.3.0",
    "headroom.js": "0.12.0",
    "js-cookie": "^3.0.5",
    "moment": "2.29.4",
    "node-cron": "^3.0.3",
    "node-fetch": "^3.3.2",
